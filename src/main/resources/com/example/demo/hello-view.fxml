<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="390.0" prefWidth="571.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <TextArea fx:id="textArea" editable="false" focusTraversable="false" prefHeight="219.0" prefWidth="531.0" />
   <TextField fx:id="textField" onAction="#onSendPress" prefHeight="25.0" prefWidth="491.0" promptText="Insert text here" />
   <Button mnemonicParsing="false" onAction="#onSendPress" text="Send" />

</VBox>
